<h1>Edit</h1>

@model Category

<form method="post">
    

    <div class="mb-3 row p-1">
        <label asp-for="TypeOfBreed" class="p-0"></label>
        <input asp-for="TypeOfBreed" class="form-control" />
        <span asp-validation-for="TypeOfBreed" class="text-danger"></span>

    </div>

    <div class="row">
        <div class="col-6 col-md-3">
            <button type="submit" class="btn btn-primary form-control">Update</button>
        </div>
        <div class="col-6 col-md-3">
            <a asp-controller="Category" asp-action="Index" class="btn btn-outline-secondary border form-control">
                Back to list
            </a>
        </div>

    </div>

</form>
@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}

